{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 80,
    "htmlWhitespaceSensitivity": "ignore",
    "plugins": [
        "prettier-plugin-ejs"
    ],
    "overrides": [
        {
            "files": "*.ejs",
            "options": {
                "parser": "html"
            }
        }
    ]
}